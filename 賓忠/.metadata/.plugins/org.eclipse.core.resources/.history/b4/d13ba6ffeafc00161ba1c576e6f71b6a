package 例外處理.登入;
import java.util.Scanner;
public class SignIn {
	public static Scanner input = new Scanner(System.in);
	public static String account = input.next();
	public static String password = input.next();
	
	
	
public static void main(String[] args){
	try{
		Check.checkinformation(accountinput(),passwordinput());
	}catch(Accounterror | Passworderror | Allerror ex){
		System.out.println("");
	}
	
}
static String accountinput(){
	System.out.println("input your account: ");
	return account;
}
static String passwordinput(){
	System.out.println("input your password: ");
	return password;
}

}

