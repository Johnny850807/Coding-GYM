package 例外處理.登入;
import java.util.Scanner;
public class SignIn {
	private static String A = "123";
	private static String B = "567";
	static Scanner input = new Scanner(System.in);
	public static String account = input.next();
	public static String password = input.next();
	
	
	
public static void main(String[] args){
	if(A != account|| B != password){ 
		if(A!=account && B == password)
		{
			System.out.println("帳號不符合");
		}
		else if(A==account && B != password)
		{
			System.out.println("密碼不符合");
		}
		else if(A!=account && B != password)
		{
			System.out.println("帳密不符合");
		}
	}
	else if(A == account&& B == password){
		System.out.println("登入成功");
	}
	
	
}
}

