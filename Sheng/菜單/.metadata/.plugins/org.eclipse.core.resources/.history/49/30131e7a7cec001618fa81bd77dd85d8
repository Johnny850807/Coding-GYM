package 垫虫;

import java.util.ArrayList;

public class Main {
	public static void main(String[] argv)
	{
		Location[] locations = new Location[5];//代刚
		Shop[] shops = new Shop[5];//代刚
		Food[] foods = new Food[5];//代刚
		
		//@\
		foods[0] = new Food("fXqjQ难");
		foods[1] = new Food("KN_q难");
		ArrayList<Item> temp = new ArrayList<Item>();
		temp.add(foods[0]);
		temp.add(foods[1]);
		shops[0] = new Shop("qjQ难\]",temp);
		
		foods[0] = new Food("KOψ");
		foods[1] = new Food("KO睫");
		ArrayList<Item> temp1 = new ArrayList<Item>();
		temp1.add(foods[0]);
		temp1.add(foods[1]);
		shops[1] = new Shop("KO\恨",temp1);
		
		ArrayList<Item> temp3 = new ArrayList<Item>();
		temp3.add(shops[0]);
		temp3.add(shops[1]);
		locations[0] = new Location("@\",temp3);
		System.out.print(locations[0].print());
		
	}
}
