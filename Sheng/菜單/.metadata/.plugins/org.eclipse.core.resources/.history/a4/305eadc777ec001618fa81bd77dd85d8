package 菜單;

import java.util.ArrayList;

public class Main {
	public static void main(String[] argv)
	{
		Location[] locations = new Location[4];
		
		Shop[] shops = new Shop[2];
		Food[] foods = new Food[2];
		foods[0] = new Food("番茄義大利麵");
		foods[0] = new Food("焗烤起司麵");
		shops[0] = new Shop("義大利麵餐館");
		shops[1] = new Shop("鐵板餐管");
		locations[0] = new Location("一餐");
		
	}
}
