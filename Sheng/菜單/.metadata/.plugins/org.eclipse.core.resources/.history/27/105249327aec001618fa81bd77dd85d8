package 垫虫;

import java.util.ArrayList;

public class Main {
	public static void main(String[] argv)
	{
		ArrayList<Item> temp = new ArrayList<Item>();
		Location[] locations = new Location[5];//代刚
		Shop[] shops = new Shop[5];//代刚
		Food[] foods = new Food[5];//代刚
		
		//@\
		foods[0] = new Food("fXqjQ难");
		foods[1] = new Food("KN_q难");
		temp.add(foods[0]);
		temp.add(foods[1]);
		shops[0] = new Shop("qjQ难\]",temp);
		temp.clear();
		
		foods[0] = new Food("KOψ");
		foods[1] = new Food("KO睫");
		temp.add(foods[0]);
		temp.add(foods[1]);
		shops[1] = new Shop("KO\恨",temp);
		temp.clear();
		
		temp.add(shops[0]);
		temp.add(shops[1]);
		locations[0] = new Location("@\",temp);
		System.out.println(locations[0].print());
		
	}
}
