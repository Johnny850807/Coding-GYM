package ¦¨ÁZ;

public class Main {
	public static void main(String argv[])
	{
		int[] ran = new int[4];
		for(int i=0;i<4;i++)
		{
			ran[i] = (int)(Math.random()*100);
		}
		Chinese chinese = new Chinese(ran[0],ran[1],ran[2],ran[3]);
		chinese.calculate();
		chinese.display();
		for(int i=0;i<4;i++)
		{
			ran[i] = (int)(Math.random()*100);
		}
		English english = new English(ran[0],ran[1],ran[2],ran[3]);
		english.calculate();
		english.display();
		for(int i=0;i<4;i++)
		{
			ran[i] = (int)(Math.random()*100);
		}
		Code code = new Code(ran[0],ran[1],ran[2],ran[3]);
		code.calculate();
		code.display();
	}
}
