package 二十一點遊戲;

public class TestProgram {
	public static void main(String[] argv)
	{
		Deck deck = new Deck();
		deck.Shuffle();
		deck.sort();
		System.out.println(deck.print());
		System.out.println("");
		System.out.println(deck.pop().print());
		System.out.println(deck.pop().print());
		System.out.println(deck.pop().print());
		System.out.println(deck.pop().print());
		System.out.println(deck.pop().print());
		System.out.println(deck.print());
	}
}
